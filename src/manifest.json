{
  "manifest_version": 3,
  "name": "Startpage Maps",
  "version": "1.1",

  "icons": {
    "16": "/icon/16.png",
    "32": "/icon/32.png",
    "48": "/icon/48.png",
    "128": "/icon/128.png"
  },
  
  "content_scripts": [{
      "matches": ["https://www.startpage.com/sp/search/*", "https://www.startpage.com/do/search*"],
      "js": ["content.js"],
      "css": ["style.css"]
  }],

  "permissions": [
    "activeTab"
  ]
}